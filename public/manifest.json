{
  "manifest_version": 3,
  "name": "Denisinside Wallet",
  "version": "1.0.0",
  "description": "A simple cryptowallet app made for educational purposes.",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "background": {
    "service_worker": "./static/js/background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab"
  ]
}